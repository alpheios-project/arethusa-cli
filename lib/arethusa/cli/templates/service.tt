"use strict";

angular.module('<%= namespaced_name(true) %>').service('<%= name(true) %>', [
  'state',
  'configurator',
  function(state, configurator) {
    var self = this;
    this.name = '<%= namespaced_name(true, true) %>';

    this.defaultConf = {
      template: 'templates/<%= namespaced_name(false, true) %>/<%= name %>.html'
    };

    function configure() {
      configurator.getConfAndDelegate(self);
    }

    this.init = function() {
      configure();
    };
  }
]);
