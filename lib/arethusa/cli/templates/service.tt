"use strict";

angular.module('<%= namespaced_name(true) %>').service('<%= name(true) %>', [
  'state',
  'configurator',
  function(state, configurator) {
    var self = this;

    this.defaultConf = {
      name: '<%= name(true) %>',
      template: 'templates/<%= namespaced_name %>/<%= name %>.html'
    }

    function configure() {
      configurator.getConfAndDelegate('<%= name(true) %>', self);
    }

    configure();

    this.init = function() {
      configure();
    };
  }
]);
